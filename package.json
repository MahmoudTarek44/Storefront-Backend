		"jsonwebtoken": "^9.0.0",
		"pg": "^8.5.1"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/express": "^4.17.9",
		"@types/jasmine": "^4.3.1",
		"@types/jsonwebtoken": "^9.0.0",
		"@types/pg": "^7.14.7",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"concurrently": "^7.6.0",
		"db-migrate": "^0.11.13",
		"db-migrate-pg": "^1.2.2",
		"eslint": "^8.30.0",
